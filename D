echo Cleaning up temp directories...

:: Delete temp files from the system and user temp folders
del /q /f /s "%TEMP%\*" >nul 2>&1
del /q /f /s "C:\Windows\Temp\*" >nul 2>&1

:: Optional: Remove folders inside TEMP (uncomment if needed)
:: for /d %%x in ("%TEMP%\*") do rd /s /q "%%x"
:: for /d %%x in ("C:\Windows\Temp\*") do rd /s /q "%%x"

echo Temp folders cleaned.

:: Now run your scripts (example script lines below)
echo Running your scripts...
call "C:\Path\To\YourScript1.bat"
call "C:\Path\To\YourScript2.bat"

echo Done!
pause
